<div class="col-md-8 col-md-offset-2" id="edit_page" >
  <div class="row">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h1>Edit the Picture</h1>
      </div>
      <div class="panel-body">
        <div class="current_image">
          <strong class="center">Right Now
            <%= image_tag @pic.image.url(:medium) %>
          </strong>
        </div>
        <%= render 'form' %>
      </div>
    </div>
  </div>
</div>


